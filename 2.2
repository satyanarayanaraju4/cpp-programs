#include <iostream>
using namespace std;
inline int add(int a, int b) {
    return a + b;
}
inline int square(int x) {
    return x * x;
}
int main() {
    int num1, num2;
    cout << "Enter two numbers: ";
    cin >> num1 >> num2;
    cout << "Sum of " << num1 << " and " << num2 << " is: " << add(num1, num2) << endl;
    cout << "Square of " << num1 << " is: " << square(num1) << endl;
    cout << "Square of " << num2 << " is: " << square(num2) << endl;
    return 0;
}
